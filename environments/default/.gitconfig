[user]
	name = Andreas Marr
	email = monolar@monolar.org
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
	ui = true
[core]
	editor = subl -n -w
	excludesfile = ~/.global_gitignore
[mergetool]
	keepBackup = true
[push]
	default = simple
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
